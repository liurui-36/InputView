<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="InputView">
        <attr name="itemSize" format="integer" />
        <attr name="itemPadding" format="dimension" />
        <attr name="itemWidth" format="dimension" />
        <attr name="itemHeight" format="dimension" />
        <attr name="border" format="dimension" />
        <attr name="rectColor" format="color" />
        <attr name="underLineColor" format="color" />
        <attr name="fillColor" format="color" />
        <attr name="textSize" format="dimension" />
        <attr name="textColor" format="color" />
        <attr name="cursorFlashTime" format="integer" />
        <attr name="isCursorEnable" format="boolean" />
        <attr name="cipherEnable" format="boolean" />
        <attr name="cursorColor" format="color" />
        <attr name="mode" format="enum">
            <enum name="underline" value="0" />
            <enum name="rect" value="1" />
            <enum name="fill" value="2" />
        </attr>
    </declare-styleable>
</resources>